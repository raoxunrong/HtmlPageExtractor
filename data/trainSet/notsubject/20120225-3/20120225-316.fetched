function vote(flag,type){
	if($.cookie(flag) == 1){
		alert("您已经投过票了，谢谢您的参与！");
		return false;
	}else{
		var url = 'http://comment.chinanews.com/iframe/special_votes_save_pk.php';
		var data = 'flag=' + flag + '&do=update&type=' + type + "&charset=gbk";
		$.ajax({
			url:url,
			data:data,
			type:"GET",
			dataType:"jsonp",
			timeout:20000
		});

		var num = parseInt($("#vote_hidden_" + type + "_" + flag).val()) + 1;
		$("#vote_" + type + "_" + flag).html(num);
		$("#vote_hidden_" + type + "_" + flag).attr('value',num);

