function Bm(data,bid,mid,adModelId){  
    if(!data) return false; 
    this.data = data;
    this.b = document.getElementById(bid);    
    this.m = document.getElementById(mid);
    this.b.innerHTML = '';
    var op = new Option('选品牌',-1);     
    this.b.options.add(op);  
    var len = this.data.length;  
    var curBrand = null;  
    for(var i=0;i<len;i++){
              var bd = this.data[i]; 
              var op = new Option(bd.n,bd.i);  
              this['k_'+bd.i] = bd;     
              this.b.options.add(op);
              if(adModelId&&curBrand==null&&this.mIn(adModelId,bd)){
                    curBrand = bd;
              }
    }
    if(curBrand){ this.b.value = curBrand.i;this.initsm(curBrand.i,adModelId); }else{
         this.initsm();
    }
   
           
    var that = this;   
    this.b.onchange = function(){that.initsm(this.value)}                    
}
Bm.prototype = {    
    mIn : function(mid,bd){
         var len = bd.s.length
         for(var i=0;i<len;i++){
                      var dss = bd.s[i];
                      var b = dss.b; 
                      var jlen = b.length;
                      for(var j=0;j<jlen;j++){
                           if(b[j].i == mid){
				return true;
			}  
                      }
                 }
           return false;
    },         
    initsm : function(v,mt){                         
         this.m.innerHTML = '';   
         var op = new Option('选车型',-1);    
         this.m.options.add(op); 
         if(!v||v==-1){ return} 
         var ds = this['k_'+v]; 
         if(ds&&ds.s){  
                var len = ds.s.length;  
                for(var i=0;i<len;i++){
                      var dss = ds.s[i];
                      var group = document.createElement('optgroup');
                      group.label = dss.n;
                      this.m.appendChild(group); 
                      var b = dss.b; 
                      var jlen = b.length;
                      for(var j=0;j<jlen;j++){
                             var op = new Option(b[j].n,b[j].i);
                             this.m.options.add(op);  
                      }
                 }
                 if(mt)this.m.value = mt;
          }   
      }     
}

