        <script type="text/javascript"> 
        var tabCache = null;
function bindingTabEvent()
{
	var oModelTabs = document.getElementById("modelTabs");
	var oTabs = oModelTabs.getElementsByTagName("a");
 
	var oModelList = document.getElementById("modelList").childNodes;
	var oModelLists = [];
	
	for(var i=0, len=oModelList.length; i<len; i++)
	{
		if(oModelList[i].nodeType == 1)
		{
			oModelLists.push(oModelList[i]);
		}
	}
	
	for(var i=0, len=oTabs.length; i<len; i++)
	{
		if(oTabs[i].parentNode && oTabs[i].parentNode.className.indexOf("tab") >= 0 )
		{
			if(oTabs[i].parentNode.className.indexOf("current") >= 0)
			{
				oModelLists[i].style.display = "block";
				tabCache = i;
			}
			else
			{
				oModelLists[i].style.display = "none";
			}
			
			oTabs[i].parentNode.onmouseover = function(i)
			{
				return function(){
					if(oTabs[i].parentNode.className.indexOf("current") >= 0)
					{
						return;	
					}
					oTabs[i].parentNode.className = "tab current";
					oModelLists[i].style.display = "block";
					if(tabCache != null)
					{
						oModelLists[tabCache].style.display = "none";
						oTabs[tabCache].parentNode.className = "tab";
					}
					tabCache = i;
				};
			}(i);
		}
	}
}
bindingTabEvent();
</script>
