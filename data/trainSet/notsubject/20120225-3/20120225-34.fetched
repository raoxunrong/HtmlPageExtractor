	function dooSubmit1() 
	{
		var bodywork= document.getElementById("bodywork").value;
	    var price=document.getElementById("price").value;
		if (price=="01" && bodywork=="00")
	    	alert("请正确选择查询条件！")
		else
		{  	
			var query=''; 
			if(bodywork != "00" && bodywork.length>0)
		       	query="bodyWork:"+bodywork+",false,and"	;	
		    if(price!="01" && price.length>0)
		    {
			 	if(query=='')
					query="price:"+price+",true,and";
				else
					query=query+"|"+"price:"+price+",true,and";
			}		
	  	
			var oSearchPirce = document.getElementById("searchPirce");
			oSearchPirce.action = "http://db.auto.sohu.com/searchTerm.sip?item="+escape(query)+"&pageNo=1&module=AutoModel&type=term&sortFlag=hot_asc";
			oSearchPirce.submit();	
		}
	}	
	function dooSubmit3(type,id1,id2,fromid) 
	{
		
		var obrand=document.getElementById(id1);
		var omodel=document.getElementById(id2);
		var oSBL = document.getElementById(fromid);
		if(obrand.value == -1){ alert("请正确选择品牌！");}
		else if(omodel.value == -1){ 
                    if(type != 1){
                     alert("请正确选择车型！");
                    }else{  
                      
                        oSBL.action = "http://db.auto.sohu.com/brand_"+obrand.value+"/index.shtml";
			oSBL.submit();
                 
                    }
                }
		else
		{
			var mID = omodel.value;
			if(mID.length >= 4){ 
				
				if(type === 1)
				{
					if(typeof adModelUrl != "undefined" && adModelUrl != "" &&mID==adModelId
)
					{
						oSBL.action = adModelUrl;
						oSBL.method = "get";
					}
					else
					{
						oSBL.action = "http://db.auto.sohu.com/model-"+ mID +".shtml";
					}
					oSBL.submit();
				}
				else if(type === 2)
				{
					oSBL.action = "http://db.auto.sohu.com/model_"+ mID +"/price.shtml";
					oSBL.submit();
				}
				else if(type === 3)
				{
					if(typeof model_forum["M" + mID] != "undefined")
					{
						oSBL.action = model_forum["M" + mID]; 
						oSBL.submit();
					}
				}
			}
		}

	} 
