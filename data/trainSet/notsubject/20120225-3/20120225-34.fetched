         var obj = localObject.getCodeObject('010');
         if(obj){
               localCode = obj.cityCode;
               localName = obj.cityName;
               prefix = obj.prefix;
         }
   } 
   if(localCode){
        var doc = document.getElementById(aid);   
        if(prefix){
        doc.href = 'http://'+prefix+'.auto.sohu.com/';
        }else{
        doc.href = 'http://auto.sohu.com/'+localCode+'/'; 
        }
        doc.innerHTML = localName+'站';
        localObject.onLocation(localCode);  
   }
}   
