/**
 *	排序，和arrayZj的数据结构相关
 *  @add 20100419 kangw
 */
function sortSPIF(arrayZj) {
	if(arrayZj) {
		var tmparySort=[];	//用于排序的构造数组
		for(var i=0;i<arrayZj.length;i++) {
			var elet=arrayZj[i];
			var obj={};
			obj.realVal=elet;
			if(elet[14]=='thismonth') {
				obj.sortVal=8001;
			} else if(elet[14]=='nextmonth') {
				obj.sortVal=8002;
			} else if(elet[14]=='nextseason') {
				obj.sortVal=8003;
			} else if(elet[14]=='nexthalfyear') {
				obj.sortVal=8004;
			} else {
				obj.sortVal=(elet[14]+'').substring('IF'.length);
			}
			tmparySort.push(obj);
		}
		//sort
		tmparySort.sort(function(left,right){
			var a=left.sortVal;
			var b=right.sortVal;
			return a>b ? 1 : (a<b ? -1 : 0)
		});
		arrayZj=[];
		for(var i=0;i<tmparySort.length;i++) {
			arrayZj.push(tmparySort[i].realVal);
		}
		return arrayZj;
	}
	return [];
}
