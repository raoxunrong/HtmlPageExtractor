		var percent = quoteData['percent'];
		percent>0 ? (elem.className = 'cRed') : (percent<0 ? (elem.className = 'cGreen') : '');
		elem.innerHTML = turnover;	  
	} catch(ex) {
	  elem.innerHTML = quoteData["turnover"];
	}
});
