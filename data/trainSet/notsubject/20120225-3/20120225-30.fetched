function isUpdate() {
	var o=document.getElementById('jsUpdate')||null;
	if (o) {
		document.body.removeChild(o);
	}
	var spt=document.createElement('script');
	spt.id='jsUpdate';
	spt.src='http://news.sina.com.cn/iframe/728/2012/0207/49/isUpdate.js?'+Math.random();//JS文件路径
	document.body.appendChild(spt);

	if (!+'\v1') {//IE
		spt.onreadystatechange=function () {
			if (this.readyState=='loaded' || this.readyState=='complete') {
				show_div(0)
				show_div(1)
			}
		}
	}else {
		spt.onload=function () {
			show_div(0)
			show_div(1)
		}
	}
}

function show_div(n) {
	var div_isUpdate=[document.getElementById('isUpdate1'),document.getElementById('isUpdate2')];
	if (isUpdate.status[n]==null) {
		isUpdate.status[n]=last_update[n];
	}
	if (last_update[n] && isUpdate.status[n] != last_update[n]) {//记录有变化
		div_isUpdate[n].style.display='block';
		isUpdate.status[n]=last_update[n];
	}
}
window.onload=function () {
	isUpdate();
	var timer_update=setInterval(isUpdate,60*1000);//每隔60秒检查一次
}

