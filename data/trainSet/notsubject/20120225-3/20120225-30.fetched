			pagesize:this.perNum,				//每页显示的页码数
			count:this.data[1].length,						//数据条数
			current:1,					 //当前页码
			displaynum:3,			//中间显示页码数
			displaylastNum:1,		//最后显示的页码数
			paging:null    
		})
	},
	createPage : function (_this,s) {
                if (s.count<1) {
			return ;
		}

		var pageCount=0;
		//计算总页码
		pageCount=(s.count/s.pagesize<=0)?1:(parseInt(s.count/s.pagesize)+((s.count%s.pagesize>0)?1:0));
		s.current=(s.current>pageCount) ? pageCount : s.current
		//循环生成页码
		var strPage="";
		//创建上一页
		if(s.current<=1){
			strPage+='<span class="prev disabled"  title="没有了">上一页</span>';
		}else{
			strPage+='<a href="javascript:;" class="prev" title="上一页"  pid="'+(s.current-1)+'">上一页</a>';
		}
		//开始的页码
		var startP=1;
		var anyMore=parseInt(s.displaynum/2);//页码左右显示最大页码数
		//结束的页码
		var endP=(s.current+anyMore)>pageCount?pageCount:s.current+anyMore;

		//可显示的码码数(剩N个用来显示最后N页的页码)
		var pCount=s.pagesize-s.displaylastNum;
		if(s.current>s.displaynum){//页码数太多时，则隐藏多余的页码
			startP=s.current-anyMore;
			for(i=1;i<=s.displaylastNum;i++){
					strPage+='<a href="javascript:;" pid="'+i+'">'+i+'</a>';
			}
			strPage+="...";
		}
		if(s.current+s.displaynum<=pageCount){//页码数太多时，则隐藏前面多余的页码
			endP=s.current+anyMore;
		}else{
			endP=pageCount;
		}
		for(i=startP;i<=endP;i++){
			if(s.current==i){
				strPage+='<span class="current">'+i+'</span>';
			}else{
				strPage+='<a href="javascript:;" pid="'+i+'">'+i+'</a>';
			}
		}
		if(s.current+s.displaynum<=pageCount){//页码数太多时，则隐藏后面多余的页码
			strPage+="...";
			for(i=pageCount-s.displaylastNum+1;i<=pageCount;i++){
				strPage+='<a href="javascript:;" pid="'+i+'">'+i+'</a>';
			}
		}
		//创建下一页
		if(s.current>=pageCount){
			strPage+='<span class="next disabled" title="没有了">下一页</span>';
		}else{
			strPage+='<a href="javascript:;" class="next" title="下一页" pid="'+(s.current+1)+'">下一页</a>';
		}
		_this.innerHTML=strPage;
		var links=_this.getElementsByTagName('a');
		var self=this;
		for (var i=0; i<links.length; i++) {
			links[i].onclick=function (e) {
				s.current=parseInt(this.getAttribute ('pid')); 		
				self.createPage(_this,s);
				self.gotoPage(s.current);
				return false;
			}
		}
		return this;
	},
	gotoPage : function (n) {
		var m= SubShow_01 ? SubShow_01.selectedIndex : 0;
		for (var i=0; i<this.pages[m].length; i++) {
			this.pages[m][i].style.display='none'
		}
		this.pages[m][n-1].style.display='block';
	},

	playNext : function(){
		 this.videoIndex++;
		 if(this.videoIndex >= this.data[this.status].length){
			return ;
			this.videoIndex=0;
		 };
		 this.play(this.videoIndex);
	},
	play : function(num,flag){
	   if ( !flag&&num!=0){document.getElementById("myMovie").playVideo(-1,'');return}
		var self =this;
		 if(num >=0 && num<this.data[this.status].length){
                         sina.$("myMovie").resetParam( 1, 1, 0, 0, this.data[this.status][num].getElementsByTagName('a')[0].getAttribute('href'),this.data[this.status][num].getElementsByTagName('a')[0].innerHTML,'');
			sina.$("myMovie").playVideo(this.data[this.status][num].getAttribute('vid'), 6, this.data[this.status][num].getAttribute('isAd'));
			this.setVideoIndex(num);

			sina.$('video_title').innerHTML=this.data[this.status][num].getElementsByTagName('span')[0].innerHTML;

			for (var i=0; i<this.cmtData[0].length; i++) {
				this.cmtData[0][i].className=this.cmtData[0][i].className.replace(/on/gi,'');
			}
			for (var j=0; j<this.cmtData[1].length; j++) {
				this.cmtData[1][j].className=this.cmtData[1][j].className.replace(/on/gi,'');
			}
			var currentCmt=this.cmtData[this.status][num];
			currentCmt.className+=' on';

			var currentPage=Math.ceil((num+1)/this.perNum);

			//播放时自动翻页
			if (num % this.perNum ==0) {
				//this.gotoPage(currentPage)

				if (this.status==0) {
					var page_box=sina.getByClass('page',sina.$(this.playListId[0]))[0];
				}else {
					var page_box=sina.getByClass('page',sina.$(this.playListId[1]))[0];
				}
				this.createPage(page_box,{
					listId : this.status,		
					pagesize:this.perNum,				//每页显示的页码数
					count:this.data[this.status].length,						//数据条数
					current:currentPage,					 //当前页码
					displaynum:3,			//中间显示页码数
					displaylastNum:1,		//最后显示的页码数
					paging:null    
				});
			}

				//评论数 2011/1/4
			var cmt_link=sina.getByClass('cmt_link',currentCmt)[0];
			if (!cmt_link || !cmt_link.href.match(/newsId=([^&]+)/gi)) return ;
			var newsId=cmt_link.href.match(/newsId=([^&]+)/gi)[0].replace(/newsId=/gi,'');
			var channel=cmt_link.href.match(/channel=([^&]+)/gi)[0].replace(/channel=/gi,'');

			sina._getJsData('http://counter.sina.com.cn/querylist?format=js&entry=g_clist&id=0&key='+channel+':'+newsId+':0',function () {
				if (g_clist && g_clist[0]) {
					cmt_link.getElementsByTagName('span')[0].innerHTML=g_clist[0][1];
				}else {
					cmt_link.getElementsByTagName('span')[0].innerHTML=0;
				}
			});

			//播放量
			var popNum=sina.getByClass('pop_num',currentCmt)[0];
			var vid=this.data[this.status][num].getAttribute('vid');

			sina._getJsData('http://l.you.video.sina.com.cn/app/count/getFlashCount.php?vid=' + vid + '-' + vid + '&action=nc',function () {
				if (count) {
					popNum.innerHTML=count[vid];
				}else {
					popNum.innerHTML=0;
				}
			});

		 };
	},
	setVideoIndex : function(num){
		var n=this.status == 0 ? 1 : 0;
		if (this.data[n][this.videoIndex]) {
			this.data[n][this.videoIndex].className=this.data[n][this.videoIndex].className.replace(/selected/gi,'');
		}
		for(var i=0;i<this.data[this.status].length;i++){
			if(num==i){
				this.data[this.status][i].className += " selected";
				this.videoIndex = num;
			}else{
				this.data[this.status][i].className =this.data[this.status][i].className.replace(/selected/gi,'');
			};
		};
	}
};

