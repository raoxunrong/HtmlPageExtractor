function getCookie(a){
	var b;
	b=a+"=";
	offset=document.cookie.indexOf(b);
	if(offset!=-1){
		offset+=b.length;
		end=document.cookie.indexOf(";",offset);
		if(end==-1){
			end=document.cookie.length
		}
		return document.cookie.substring(offset,end)
	}
	else{
		return""
	}
}
