					if (t==this.div) {
						return ;
					}else if(t==document.documentElement){
						this.hide()
						return ;
					}else {
						t=t.parentNode;
					}
				}while(t.parentNode);
			},
			toggles : function () {
				this.isShow ? this.hide() : this.show();
			}
		}
