					}
					listcontent += '<li id="sinaNote_MenuItem_' + key + '_' + _rndID + '" title="'+itemLabel+'">' + itemLabel + '</li>';
				}
			}
			$E("sinaNote").innerHTML = listcontent;
			for (var i = 0; i < username.length; i++) {
