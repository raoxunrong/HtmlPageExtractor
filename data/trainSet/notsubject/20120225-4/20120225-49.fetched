<style type="text/css">
/* 初始化CSS */
html, body, ul, li, ol, dl, dd, dt, p, h1, h2, h3, h4, h5, h6, form, fieldset, legend,img{margin:0;padding:0;}
fieldset,img{border:none;}
address,caption,cite,code,dfn,th,var{font-style:normal;font-weight:normal;}
ul,ol{list-style:none;}
select,input{vertical-align:middle;}
select,input,textarea{font-size:12px;margin:0;}
table{border-collapse:collapse;}
body{color:#333;padding:5px 0;font:12px/20px "宋体";}

.clearfix:after{content:".";display:block;height:0;visibility:hidden;clear:both;}
.clearfix{zoom:1;}
.clearit{clear:both;height:0;font-size:0;overflow:hidden;}

a{color:#009;text-decoration:none;outline:none;}
a:visited{color:#800080;}
a:hover, a:active, a:focus{color:#f00;text-decoration:underline;}

h1, h2, h3, h4, h5, strong { font-weight:normal; }
html{background:#041c41 url(http://i0.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_bg.jpg) repeat-x}
body{background:url(http://i1.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_body_bg.jpg) no-repeat 50% 0;}
.wrap { width:950px; margin:0 auto; }
.banner { background:url(http://i0.sinaimg.cn/video/deco/2011/1205/images/sports_1130_zyc_banner.jpg) no-repeat; height:355px;}
.banner div { padding:0px 44px;}
.banner a,.banner a:visited { color:#d4edff; padding:0px 14px; line-height:42px;}
.banner img{ display:block}
/* ============== 横切定义 ============== */
.part_01 { background:url(http://i0.sinaimg.cn/video/deco/2011/1205/images/sports_1130_zyc_back01.jpg) no-repeat; height:471px;}
.part_01 .p_left{float:left; width:504px; padding:16px 0 0 30px;}
.part_01 .p_right{float:right; width:364px;}
.part_01 .p_right img{margin-left:10px;cursor:pointer;}

.part_02 { background:url(http://i1.sinaimg.cn/video/deco/2011/1205/images/sports_1130_zyc_back02.jpg) no-repeat; height:491px; margin-top:10px;}
.part_02 .p_left{float:left; width:379px; padding:0 10px; padding-top:36px;}
.part_02 .p_right{float:left; padding-top:63px;}

.part_03 { background:url(http://i2.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_03.jpg) no-repeat; height:407px; margin-top:10px;}
.part_03 .p_cont { padding:0 10px;}

.part_04 { background:url(http://i3.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_04.jpg) repeat-y; zoom:1;margin-top:10px;}
.part_04 .p_left{float:left; width:506px;}
.part_04 .p_right{float:left; width:414px; padding:10px;}
.part_04 .p_bot{ background:url(http://i3.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_04.jpg) no-repeat -2000px 0; height:20px; overflow:hidden; clear:both;}

/**--------------添加横切 begin--------------------**/
.part_05{margin-top:10px;}
.part_05 .tit_05{background:url(http://i2.sinaimg.cn/ty/deco/2011/1208/images/part_hd_wqhg_dc.jpg) no-repeat left top;}
.part_05 .tit_05 a{float:right;margin-right:60px;font-size:14px;}
.part_05 .p_cont{padding:10px 20px 0;overflow:hidden;background:url(http://i2.sinaimg.cn/ty/deco/2011/1208/images/part_bd_dc.jpg) repeat-y;}
.part_05 .p_cont li{float:left;width:130px;height:145px;text-align:center;}
.part_05 .p_cont a span{height:auto;line-height:120%;}
.part_05 .p_cont a:link img{width:115px;height:85px;padding:3px;border:1px solid #d2d2d2;margin-bottom:7px;}
.part_05 .p_cont a:visited img{width:115px;height:85px;padding:3px;border:1px solid #d2d2d2;margin-bottom:7px;}
.part_05 .p_cont a:hover img{border:1px solid #77caff;background:#daf8ff;}
.part_05 .p_bot{ background:url(http://i2.sinaimg.cn/ty/deco/2011/1208/images/part_ft_dc.jpg) no-repeat left bottom; height:19px; overflow:hidden; clear:both;}
/**--------------添加横切 end--------------------**/

/* ================== block define ================== */
.pcont01{ padding:6px 5px 0 14px}
.pcont01 p{ color:#012a4d; text-indent:1em; background:url(http://i0.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_05.png) no-repeat 7px 12px; font-size:14px; line-height:24px;}
.pcont02{ padding:15px 0 0}

/* 板块单独定义 */
.blk_01{ padding:65px 37px 0 0px; color:#fff; line-height:24px;}
