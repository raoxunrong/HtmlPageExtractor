.blk_01 span a{color:#FFFFFF;}
.blk_02 { padding:13px 0 0 25px; }
.blk_02 .pl_line{background:url(http://i0.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_05.png) no-repeat 7px 12px;text-indent:1em; font-size:14px; line-height:24px; color:#012a4d}
.blk_03{ background:#f4f4f4; padding:15px}
.blk_03 .info{ padding-bottom:8px;}
.blk_03 .info input{border:solid 1px #cacaca; background:#fff;}
.blk_03 .form .btn{ text-align:center; padding:10px 0 0}
.blk_03 .form textarea{ border:solid 1px #cacaca; background:#fff; padding:10px; height:92px; width:365px;}
.blk_03 .form .submit{ background:url(http://i1.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_06.png) no-repeat; width:66px; height:27px; border:none; cursor:pointer; font-size:14px; color:#006bb6;}
.blk_03 .form .all{ background:url(http://i1.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_06.png) no-repeat -100px 0; width:76px; height:27px; margin-left:20px; border:none; cursor:pointer; font-size:14px; color:#006bb6;}
.blk a:visited{ color:#800080;}
.blk a:hover{color:#c00;}
/* ================= commen define ================ */

/* 标题 */
.tit{height:63px}
.tit h2 { text-indent:-999em}
.tit_03 { background:url(http://i3.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_04.jpg) no-repeat -1000px 0;}
/* 图文之上图下文 */
.blk_tw_pic {zoom:1; text-align:center;}
.blk_tw_pic img { border:solid 1px #000; margin:0 auto; background:#fff; display:block; }
.blk_tw_pic img.p1 {padding:1px}
.blk_tw_pic img.p2 {padding:2px}
.blk_tw_pic img.p3 {padding:3px}
.blk_tw_pic a:hover img { border-color:#000; }
.blk_tw_pic a span{ color:#013979; display:block; text-align:center; height:34px; line-height:28px;}
.blk_tw_pic a:visited span{ color:#800080;}
.blk_tw_pic a:hover span{ color:#c00;}

.r5{ width:33.3%; float:left;}
/* 列表 */
.list_01 { padding-top:58px;}
.list_01 li { line-height:29px; height:29px; padding-left:27px; background:url(http://i2.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_07.png) no-repeat 0 13px}
.list_01 li a,  .list_01 li a:hover { color:#d6efff }
.list_01 li a:visited { color:#d6efff;}
.list_01 li.selected { background-position:0 -34px; }
.list_01 li.selected a{ color:#000; text-decoration:none;blr:expression(onFocus=this.blur())}
.list_01 li.selected a:hover { color: #000000;text-decoration: none;}
.list_01 li.selected a:visited {color:#000; text-decoration:none;}
.list a:link{ color:#333;}
.list a:visited{color:800080;}
.list a:hover{color:#c00;}

.list_02 li { line-height:24px; height:24px; padding-left:27px; background:url(http://i2.sinaimg.cn/ty/deco/2011/0530/images/sports_nba_0530_07.png) no-repeat 0 10px}
.list_02 li a,  .list_02 li a:hover { color:#d6efff }
.list_02 li a:visited { color:#d6efff;}

