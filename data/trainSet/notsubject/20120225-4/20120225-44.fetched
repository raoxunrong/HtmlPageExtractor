<script language="javascript">
var team_id = parseInt("0",10);
var date = "2012-02-10";
var week_day = parseInt("",10);
var ajax_url = "json/ajax.php";
var last_week_day = '';
var next_week_day = '';
function getTeamScheduleWeek(){
	var pars = "_action=get_team_schedule_week&team_id="+team_id+"&date="+date+"&rnd="+parseInt(Math.random()*999999);
	var url = ajax_url;
	var myAjax = new Ajax.Request(url,{method:'get',parameters:pars,onSuccess:setTeamScheduleWeek});
}
function setTeamScheduleWeek(transport){
	var xmlRequest = transport.responseXML;
	var week_day_a = xmlRequest.getElementsByTagName("week_day");
	var day_a = xmlRequest.getElementsByTagName("day");
	var short_day_a = xmlRequest.getElementsByTagName("short_day");
	var last_week_day_a = xmlRequest.getElementsByTagName("last_week_day");
	var next_week_day_a = xmlRequest.getElementsByTagName("next_week_day");
	if(team_id>0){
		var time_a = xmlRequest.getElementsByTagName("time");
		var game_code_a = xmlRequest.getElementsByTagName("game_code");
		var is_final_a = xmlRequest.getElementsByTagName("is_final");
		var match_info_a = xmlRequest.getElementsByTagName("match_info");
	}
	else{
		var match_count_a = xmlRequest.getElementsByTagName("match_count");
	}
	last_week_day = String(last_week_day_a[0].childNodes[0].nodeValue);
	next_week_day = String(next_week_day_a[0].childNodes[0].nodeValue);
	var w_d = 0;
	var tag = '';
	
	for(var i=0;i<week_day_a.length;i++){
		w_d = parseInt(week_day_a[i].childNodes[0].nodeValue,10);
		tag = 'd_'+w_d;
		
		if(team_id>0){
			if(is_final_a[i].childNodes[0].nodeValue=="1"){
				$(tag).down("p").update(formatDate(short_day_a[i].childNodes[0].nodeValue));
				$(tag).down("div").update('<a href="boxscore_html/boxscore_'+game_code_a[i].childNodes[0].nodeValue+'.htm" target="_blank">'+match_info_a[i].childNodes[0].nodeValue+'</a>');
			}
			else{
				$(tag).down("p").update(formatDate(short_day_a[i].childNodes[0].nodeValue));
				$(tag).down("div").update(match_info_a[i].childNodes[0].nodeValue);
			}
		}
		else{
			$(tag).down("p").update(formatDate(short_day_a[i].childNodes[0].nodeValue));
			$(tag).down("div").update('<a href="nba_schedule_by_day.php?teamid='+team_id+'&d='+day_a[i].childNodes[0].nodeValue+'&season_year=2009#new_page_begin">'+match_count_a[i].childNodes[0].nodeValue+'场比赛</a>');
		}
		
		if( String(day_a[i].childNodes[0].nodeValue)==String(date) ){
			$(tag).addClassName('on');
		}
		else{
			$(tag).removeClassName('on');
		}
		
	}
}
function getToday(){
	var date = new Date();
	var y = date.getFullYear();
	var m = date.getMonth() + 1;
	var d = date.getDate();
	return y + "-" + m.toPaddedString(2) + "-" + d.toPaddedString(2);
}
function formatDate(date){
	var d = date.split("/");
	return d[0]+"月"+d[1]+"日";
}
function getLastWeek(){
	var pars = "_action=get_team_schedule_week&team_id="+team_id+"&date="+last_week_day+"&rnd="+parseInt(Math.random()*999999);
	var url = ajax_url;
	var myAjax = new Ajax.Request(url,{method:'get',parameters:pars,onSuccess:setTeamScheduleWeek});
}
function getNextWeek(){
	var pars = "_action=get_team_schedule_week&team_id="+team_id+"&date="+next_week_day+"&rnd="+parseInt(Math.random()*999999);
	var url = ajax_url;
	var myAjax = new Ajax.Request(url,{method:'get',parameters:pars,onSuccess:setTeamScheduleWeek});
}
function getDefaultWeek(){
	var pars = "_action=get_team_schedule_week&team_id="+team_id+"&date=&rnd="+parseInt(Math.random()*999999);
	var url = ajax_url;
	var myAjax = new Ajax.Request(url,{method:'get',parameters:pars,onSuccess:setTeamScheduleWeek});
}
Event.observe(window,"load",getTeamScheduleWeek);
