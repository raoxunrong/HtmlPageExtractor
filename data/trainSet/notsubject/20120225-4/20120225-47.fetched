<script type="text/javascript">
jQuery(function(jq){
	
	function popFun(rid){
		var imgEls = jq("#"+rid+" .pp li img");
		
		imgEls.each(function(i){
			var els = jq(this);
			var elsLI = els.parent().parent();//li
			var playPop;
			
			els.hover(function(){
				elsLI.css("z-index",1000);
				var vidDt = elsLI.find(".vidNum");//vid
				var vrsIdDd = elsLI.find(".vrsId");//vrs_id
				var subType = elsLI.find(".popInfo").attr("rel");//类型
				var vidNum = vidDt.attr("rel"), vrsId = vrsIdDd.attr("rel"), typeNum;
				
				switch (subType.toLowerCase()){
					case "mv":
						typeNum = 1;
						break;
					case "tv":
						typeNum = 2;
						break;
					case "doc":
						typeNum = 1;
						break;
					case "comic":
						typeNum = 4;
						break;
				}
				vidDt.attr("id","vid"+vidNum);
				vrsIdDd.attr("id","vrs"+vrsId);
				
				wrPoPData(typeNum, vrsId, vidNum);//读播放数、评分
				
				jq("#"+rid).find(".popInfo").attr("style","display:none");
				
				playPop = setTimeout(function(){
					elsLI.find(".popInfo").attr("style","display:block");
				},600);
				
			},function(){
				elsLI.find(".popInfo").hover(function(){
					elsLI.css("z-index",1000);
					jq(this).css("display","block");
				},function(){
					elsLI.css("z-index",1);
					jq(this).css("display","none");
				});
				clearTimeout(playPop);
				els.queue(function () {
					elsLI.css("z-index",1);
					elsLI.find(".popInfo").css("display","none");
					els.dequeue();
				});
			});
			
		});
	}
	popFun("mvPop");
	popFun("tvPop");
	popFun("dmPop");
	popFun("docPop");
	
	var wrPoPData = function(typeNum, vrsId, vidN){
		jq.getScript("http://vote.biz.itc.cn/count_v77_t"+typeNum+"_i"+vrsId+"_b_c.json", function(){
			var Mark_json = eval("productJSON_"+vrsId);
			var vs = Mark_json.votecount_77, ts = Mark_json.totalScore_77, tc = Mark_json.totalCount_77;
			wrMark = (ts / (tc != 0 ? tc : 1)).toFixed(1);
			jq("#vrs"+vrsId).html("评分：<b>"+wrMark+"</b>分");
		});
		
		jq.getScript("http://count.vrs.sohu.com/count/query.action?videoId="+vidN,function(){
			playCont = eval("count");
			jq("#vid"+vidN).html("播放：<b>"+playCont+"次</b>");
		})
	}
});
</script>

