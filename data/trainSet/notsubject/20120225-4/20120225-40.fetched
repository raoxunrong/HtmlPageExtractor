<style type="text/css">
/* ================== css reset ================== */
html, body, ul, li, ol, dl, dd, dt, p, h1, h2, h3, h4, h5, h6, form, fieldset, legend, img { margin:0; padding:0; }
fieldset, img { border:none; }
address, caption, cite, code, dfn, th, var { font-style:normal; font-weight:normal; }
ul, ol { list-style:none; }
select, input { vertical-align:middle; }
select, input, textarea { font-size:12px; margin:0; }
table { border-collapse:collapse; }
body { background:#fff; color:#666; padding:5px 0; font:12px/20px "宋体"; }
.clearfix:after { content:"."; display:block; height:0; visibility:hidden; clear:both; }
.clearfix { zoom:1; }
.clearit { clear:both; height:0; font-size:0; overflow:hidden; }
a { color:#009; text-decoration:none; }
a:hover, a:active, a:focus { color:#f00; text-decoration:underline; }
a:visited { color:#800080; }
a:hover img{border-color:#f00}
.space{height:10px; background:#fff; overflow:hidden;}
h1, h2, h3, h4, h5, h6, strong { font-weight:normal }
.wrap { width:950px; margin:0 auto; }
/* ============== 横切定义 ============== */
.logo { margin:0px 0 6px; _margin-top:-10px;}
.part_01 .part_top { height:6px; overflow:hidden; background:url(http://i2.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_02.png) no-repeat; }
.part_01 .part_bot { height:6px; overflow:hidden; background:url(http://i2.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_02.png) no-repeat 0 -6px; }
.part_01 .part_cont { border:solid 1px #e3e3e3; border-width:0 1px; padding:0 1px; }
.part_01 .part_cont .part_01_cont { background:#f9f9f9; padding:10px 10px 0; }
.part_01 .p_left { float:left; width:360px; }
.part_01 .p_right { float:right; width:565px; }
.part_02 { background:url(http://i3.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_03.png) repeat-y 0 0; margin-top:10px; }
.part_02 .p_left { width:360px; float:left; }
.part_02 .p_right { width:580px; float:right; }
.part_02 .part_bot { background:url(http://i3.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_03.png) no-repeat -950px 0; height:10px; overflow:hidden; clear:both; }
.p_right_bot{ background:#fff url(http://i3.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_03.png) no-repeat -1320px 0; height:10px; overflow:hidden; padding-bottom:10px; }
/* ============== 标题 ================= */
.tit_01 { height:30px; background:url(http://i2.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_02.png) no-repeat 100% -20px; }
.tit_01 h2 { background:url(http://i2.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_02.png) no-repeat; padding-left:40px; line-height:29px; font-size:14px ;color:#333; }
.tit_01 h2 a:link, .tit_01 h2 a:visited { color:#333; font-size:14px }
.tit_01 h2 a:hover { color:#f00; }

.tit_01 .more{float:right; padding-right:10px; line-height:29px; margin-top:-28px;}

.tit_01 .tH2_01 { background-position:-900px -60px; }
.tit_01 .tH2_02 { background-position:-900px -100px }
.tit_01 .tH2_03 { background-position:-900px -140px }
.tit_01 .tH2_04 { background-position:-900px -180px }
.tit_01 .tH2_05 { background-position:-900px -220px }
.tit_01 .tH2_06 { background-position:-900px -260px }

.tit_02{ border-bottom:solid 1px #d7d7df;}
.tit_02 h2{border-bottom:solid 2px #d7d7df; background:url(http://i2.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_02.png) no-repeat -890px -450px; padding:0 15px 0 30px; display:inline-block; font-size:14px; color:#000}
.tit_02 h2 a,.tit_02 h2 a:visited{ color:#000;}
.tit_02 h2 a:hover{ color:#f00;}
.tit_03{ background:#f3f3f5; padding:5px; margin:10px 11px 0 11px;}
.tit_03 h3{ border:solid 3px #fff; height:15px; line-height:15px; padding-left:15px; border-width:0 3px;color:#009; }
.tit_03 h3 a,.tit_03 h3 a:visited{ color:#009; font-size:14px; display:block; line-height:16px;}
.tit_03 h3 a:hover{ color:#f00;}
/* 块 */
.blk_01{padding:6px 15px 10px 20px}
.blk_01 h1{ font-size:24px; color:#006699; font-family:"微软雅黑", "黑体"; text-align:center;}
.blk_01 p.p_00{ padding-top:15px; text-indent:2em}
/* 图文 */
.blk_tw { padding:10px 20px 0 0 }
.blk_tw img { float:left; margin-right:8px; padding:4px; border:solid 1px #ccc; background:#fff; }
.blk_tw h3 a { color:#039; font-size:14px; line-height:28px; }
.blk_tw h3 a:hover { color:#f00; }
.blk_tw p { line-height:18px }
.blk_tw_1{ float:left;}
.blk_tw_1 img { padding:3px; background:#fff; border:solid 1px #d7d7df;}
.blk_tw_1 p{ text-align:center; line-height:24px}
.r2{ padding:4px 9px}
/* 列表*/
.list_01{ padding:7px 15px 10px 15px}
.list_01 li{line-height:24px; height:24px; font-size:14px; background:url(http://i2.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_02.png) no-repeat 5px -90px; padding-left:14px; zoom:1}
.list_01 li:after { content:"."; display:block; height:0; visibility:hidden; clear:both; }
.list_01 li span{float:right;}
/* footer */
.footer { line-height:24px; margin:40px; text-align:center; }
.footer, .footer a { color:#666; }
/* 其他定义 */
.p_01{color:#a0a0a0; padding:0 !important}
.p_02{color:#a0a0a0;padding:0 !important}
.p_03{text-indent:2em; padding:5px 15px 20px;}
.line_01{background:url(http://i0.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_line.png) repeat-x; height:1px; overflow:hidden; font-size:0; margin:0 10px;}
/* 表单 */
.form_01{ padding-bottom:10px;}
.form_01 input{ background:#f3f3f5; border:solid 1px #d7d7df; height:18px; _height:16px; width:118px;}
.form_01 .f_left{ float:left; }
.form_01 .f_right{ float:right; }
.form_02 .input_01{ color:#a0a0a0;float:left;background:#f3f3f5; border:solid 1px #d7d7df; width:230px; height:73px;  overflow:hidden; padding:5px;}
.form_02 .input_02{ cursor:pointer;width:70px; height:60px; background:url(http://i2.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_02.png) no-repeat 0 -150px; border:none; margin-bottom:5px;}
.form_02 .input_03{ cursor:pointer; width:70px; height:20px; background:url(http://i2.sinaimg.cn/dy/main/mediafocus/news_mtjj_zxh_02.png) no-repeat 0 -215px; border:none}
.form_02 .f_right{ float:right; width:70px;}
</style>
