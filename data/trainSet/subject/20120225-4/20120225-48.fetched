<script type="text/javascript">
//<![CDATA[
//Core
//GetObj
var getObj = function(a){
	if(document.getElementById){return eval('document.getElementById("'+a+'")')}else if(document.layers){return eval("document.layers['"+a+"']")}else{return eval('document.all.'+a)}
}

//GetTag
var getTag = function(element){
	return document.getElementsByTagName(element)
}

//Delegate
var delegate = function (fn,params,obj,name){  
     return function(){ 
         fn.call(obj||window,params,name);  
     }  
} 

//Once
var one = function(fn){ 
	return function(){ 
		try{ fn.apply(this, arguments);}catch(e){}
		finally{ fn = null;} 
	} 
}

//LoadScript
var loadScript = {
	tag:function(element){return document.getElementsByTagName(element)},
	ce:function(element){return document.createElement(element)},
	js:function(url,callback)
	{
		s = loadScript.ce('script');
		s.type = "text/javascript";
		s.onreadystatechange = ready;
		s.onerror = s.onload = callback;
		s.src = url;
		loadScript.tag('head')[0].appendChild(s);
		function ready(){
			if (s.readyState == 'loaded' || s.readyState == 'complete') {
				callback();
			}
		};
	}
}

//SetFontSize
var doZoom = function(size){
	var artibody = getObj("artibody");
	artibody.className ="blkContainerSblkCon f"+size
}

//SetPosition
var SetBgPos = function(obj,x,y){
  if(obj.style.backgroundPositionX == ''){
	  obj.style.backgroundPositionX=x;obj.style.backgroundPositionY=y;
  }else{
	  obj.style.backgroundPosition=x+' '+y;
  }
}

//SetTab
var setTab = {
	loopobj:function(){
		var newsTab = [],name='',len = arguments.length-1,pFunc;
		for(var i=0;i<arguments.length;i++){
			pFunc = function(i,name){setTab.tab(name,i,len);};
			var obj = getObj(arguments[i]);
			name = obj.id.split("_");
         	obj.onmouseover = delegate(pFunc,i,obj,name[0]);
		}
	},
	tab:function (name,cursel,n){
		for(i=0;i<=n;i++){
			var menu=document.getElementById(name+'_tab'+i);
			var con=document.getElementById("con_"+name+"_"+i);
			menu.className=i==cursel?"tab_active":"tab_normal";
			con.style.display=i==cursel?"block":"none";
		}
	} 
}

//Rank Style
var setRankStyle = {
	set:function(){
		for(var i=0;i<arguments.length;i++){
			setRankStyle.num = 0;
			var obj = getObj(arguments[i]);
			var objLi = obj.getElementsByTagName('span');
			for(var n=0;n<objLi.length;n++){
				if(objLi[n].className == 'ranknum' && setRankStyle.num < 3){
					SetBgPos(objLi[n],'-55px','-25px');
					setRankStyle.num++;
				}
			}
		}
	},
	num:0
}

//显示数据
if(getObj("cmntNum")){
getObj("cmntNum").innerHTML = Count.c_count;//留言条数
if (Count.c_count==0)getObj("cmnt_count").style.display = 'none';
}

if(getObj("loginName")){
getObj('loginName').onfocus = one(function(){
	this.value='';	
});
}

getObj('saveNews').onclick = function(){
	try{
		//保存页面
		var SinaSavePage = {
		html:'<form action="http://roll.news.sina.com.cn/savepage/save.php" method="post" target="sina_save_page_iframe" id="sina_save_page_form"><input type="hidden" name="url" value="" /></form><iframe style="display:none;" id="sina_save_page_iframe" name="sina_save_page_iframe"></iframe>',
		/**
		* @param string url url of page
		* @return void
		*/
		save:function(url){
			var form = document.getElementById('sina_save_page_form');
				if(typeof url != 'string'){url = document.location;	}
				form.elements['url'].value = url;
				form.submit();
			}
		};
		document.getElementById('savaBox').innerHTML = SinaSavePage.html;
		SinaSavePage.save();
	}catch(e){
		//loadScript.js('http://news.sina.com.cn/iframe/js/sinasavepage.js',function(){SinaSavePage.save();});
	}
	finally{ return false;} 
}
getObj('fullView').onclick = function(){
	try{
		forSFullView.FullView();
	}catch(e){
		loadScript.js('http://news.sina.com.cn/js/ui/con08/fullscreenview_090714001.js',function(){forSFullView.FullView();});
	}
	finally{ return false;} 
}

getObj('printNews').onclick = function(){
	try{
		forSPrint.Print();
	}catch(e){
		loadScript.js('http://news.sina.com.cn/js/ui/con08/print_090714001.js',function(){forSPrint.Print();});
	}
	finally{ return false;} 
}
