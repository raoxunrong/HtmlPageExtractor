function cmnt_print(){
	var limit = 5;
	var formated_cmsg = "";
	var m_content = "";
	$id('soft_cm_num').innerHTML = '评论(' + Count.c_total + '条)';
	if(CmsgList.length!=0) {
		if ( CmsgList.length <= limit ) {
			limit = CmsgList.length;
		}
		for ( var i=0; i<limit; i++ ){　
			if ( trim(CmsgList[i].m_user) == "" ) {
				var suser = "新浪用户";
			}
			else {
				var suser = CmsgList[i].m_user;
			}
			m_content = CmsgList[i].m_content;
			m_content.replace("/</g", "&lt;");
			m_content.replace("/>/g", "&gt;");
			formated_cmsg += '\
				<div style="border-bottom:1px dotted #ccc;padding:5px; margin-bottom:5px;">\
				<p style="color:#999999">' + suser + '　' + CmsgList[i].m_datetime + '</p>\
				<p>' + m_content + '</p>\
				</div>\
			';
		}
	}
	document.getElementById("show_comment_list").innerHTML= formated_cmsg;
}
